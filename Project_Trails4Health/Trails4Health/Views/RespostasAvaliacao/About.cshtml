@model IEnumerable<Trails4Health.Models.ViewModels.ViewModelAgruparPorGuia>

@{
    ViewData["Title"] = "Student Body Statistics";
}

<h2>Student Body Statistics</h2>

<table>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GuiaID)
            </td>
            <td>
                @item.ContarRespostas
            </td>
        </tr>
    }
</table>